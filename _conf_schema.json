{
    "proxy": {
        "description": "代理服务器地址，留空则不使用代理",
        "type": "string",
        "hint": "格式: http://127.0.0.1:7890 或 socks5://127.0.0.1:1080",
        "default": ""
    },
    "mosaic_level": {
        "description": "缩略图马赛克级别",
